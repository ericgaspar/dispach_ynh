[Unit]
Description=Dispach
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
User=__APP__
Group=nogroup
ExecStart=__FINAL_PATH__/bin/__APP__ -a 127.0.0.1 -p __PORT__
Restart=on-failure
RestartSec=5
StartLimitInterval=60s
StartLimitBurst=3

[Install]
WantedBy=default.target